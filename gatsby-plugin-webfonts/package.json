{
  "name": "gatsby-plugin-webfonts",
  "version": "2.3.0",
  "description": "A Gatsby plugin to handle cdn, base64 and self hosted webfonts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hupe1980/gatsby-plugin-webfonts"
  },
  "keywords": [
    "gatsby-plugin",
    "gatsby",
    "plugin",
    "web",
    "fonts",
    "loader",
    "google",
    "webfonts",
    "webfontloader"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "build: watch": "babel -w src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "jest": "jest",
    "jest:watch": "jest --watch"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0 || ^4.0.0  || ^5.0.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "cssnano": "^5.1.14",
    "fs-extra": "^10.0.1",
    "lodash.isempty": "^4.4.0",
    "postcss": "^8.4.19",
    "postcss-js": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "babel-jest": "^27.5.1",
    "babel-preset-gatsby-package": "^2.11.0",
    "cross-env": "^7.0.3",
    "jest": "^29.3.1"
  }
}
